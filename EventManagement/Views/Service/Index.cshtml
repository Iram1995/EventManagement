@model IEnumerable<EventManagement.Models.Services>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.service)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cost)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.service)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cost)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.serviceId }) |
          @*  @Html.ActionLink("Details", "Details", new { id=item.serviceId }) |*@
            @Html.ActionLink("Delete", "Delete", new { id=item.serviceId })
        </td>
    </tr>
}

</table>
